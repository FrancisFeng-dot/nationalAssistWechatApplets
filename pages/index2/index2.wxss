/**index.wxss**/

page {
  width: 100%;
  height: auto;
  overflow-y: auto;
  margin: 0;
  padding: 0;
  background: #F5F5F5;
}

.container {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  position:relative;
}

.raiders{
  width:100%;margin:0 auto;height:260rpx;
}

.raiders navigator,.raiders view{
  width:100%;height:100%;
  background-size:100% 100%;
}

.raiders button{
  width:100%;height:100%;
  background: transparent;
  background-size:100% 100%;border:0;
}

.raiders button::after{
  border:0;
}

.signanimation{
  animation: mymove1 1.5s linear infinite;
  -o-animation: mymove1 1.5s linear infinite;
  -moz-animation: mymove1 1.5s linear infinite;
  -webkit-animation: mymove1 1.5s linear infinite;
}

@keyframes mymove1 {
  0% {transform:translate(0,4px);}
  50% {transform:translate(0,-4px);}
  100% {transform:translate(0,4px);}
}
@-webkit-keyframes mymove1 {
  0% {transform:translate(0,4px);}
  50% {transform:translate(0,-4px);}
  100% {transform:translate(0,4px);}
}

.invitefriend{
  width:380rpx;height:76rpx;
  position:relative;
  background-image:url('https://qmzgcdn.boc7.net/test/inviteprom2.png');
  background-size:424rpx 116rpx;background-position: -20rpx -20rpx;
  margin:0 auto;margin-top:34rpx;
  border-radius:38rpx;
  display:flex;justify-content:center;align-items:center;
  padding:0;border:0;box-shadow:0rpx 0rpx 14rpx #FE7641;
}

.invitefriend:after{
  border:0;
}

.invitefriend>image{
width:28rpx;height:28rpx;
}

.invitefriend>text{
font-family:MicrosoftYaHei;font-size:28rpx;color:#ffffff;margin-left:8rpx;
}

.indextable{
  width: 100%;
  height:190rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #ffffff;
}

.indextable>navigator,.indextable>button{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.indextable>button{
  border:0;
  padding: 0;
  margin: 0;background: transparent;
}
.indextable>button::after{
  border:0;
}

.indextable>navigator>image,.indextable>button>image{
  width: 86rpx;
  height: 86rpx;
  border-radius: 20rpx;
  margin-left:30rpx;
  margin-right:30rpx;
}

.indextable>navigator>text,.indextable>button>text{
  font-family: MicrosoftYaHei;
  font-size:24rpx;
  color:#4D4D4D;
  margin-top: 18rpx;
  line-height:32rpx;
}

.rotor {
  width: 100%;
  height: 70rpx;
  background: #ffffff;
  display: flex;
