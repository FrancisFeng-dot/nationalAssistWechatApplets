  width:90%;
  height:70rpx;
  line-height: 70rpx;
}

.everydaylottery{
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-top: 16rpx;
}

.everydaylottery>text{
  font-size: 30rpx;
  font-family: MicrosoftYaHei;
  color: #333333;
}

.everydaylottery>navigator>text{
  font-size: 26rpx;
  font-family: MicrosoftYaHei;
  color: #666666;
}

.everydaylottery>navigator>image{
  width:24rpx;
  height: 24rpx;
  margin-left: 10rpx;
}

.prizethumb{
  background-size:100% 100%;
  height:330rpx;
  position:relative;
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
}

.prizethumb>view{
  display:flex;
  justify-content:space-between;
  align-items:center;
  width:100%;height:70rpx;
  position:absolute;bottom:0;
  font-family:SourceHanSansCN-Regular;
  color:#ffffff;
  box-sizing:border-box;
  padding:0 20rpx;
}

.prizethumb>navigator,.prizethumb>text{
  position:absolute;top:0;right:0;
  font-family:SourceHanSansCN-Regular;
  color:#ffffff;
  font-size:24rpx;
  background-image: url('https://qmzgcdn.boc7.net/test/activeSponsor1.png');
  background-size: 100% 100%;
  box-sizing:border-box;
  padding:16rpx;
  border-top-right-radius:22rpx;
  border-bottom-left-radius:22rpx;
}

.prizethumb>view>view:nth-child(1){
  display:flex;
  justify-content:flex-start;
  align-items:center;
  z-index:1;
}

.prizethumb>view>view:nth-child(3){
  background:#000000;
  opacity:0.5;
  width: 100%;
  height:100%;
  position: absolute;
  bottom:0;z-index:0;left:0;
}

.prizethumb>view>view>text,.prizethumb>view>text{
  font-size:24rpx;
  z-index:1;
}

.lotterygoodsdetail{
  display:flex;
  justify-content:space-between;
  align-items:center;
  padding:0 20rpx;
  height:130rpx;
}

.lotterygoodsdetail>view{
  display:flex;
