  line-height:56rpx;
  border-radius:28rpx;
  font-family:MicrosoftYaHei;
  font-size:26rpx;
  margin:0;
  border:0;
}

.lotterygoodsdetail>button::after{
  border:0;
}

.lotterycountdown{
  font-size:26rpx;
  margin:0 10rpx;
}

.lotterycountdown>text{
  padding:4rpx 6rpx;
  background-image:url('https://qmzgcdn.boc7.net/test/countdown_back1.png');
  background-size:100% 100%;
  margin: 0 6rpx;
}

