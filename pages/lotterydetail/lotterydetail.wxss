  justify-content: space-between;
  align-items: center;
  font-family: SourceHanSansCN-Medium;
  font-size:28rpx;
  color:#227E1E;
}
.sponsor>view:nth-child(1){
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.sponsor>view:nth-child(1)>text:nth-child(2){
  color:#ffffff;
  padding:2rpx 14rpx;
  background:#3DA74C;
  border-radius:20rpx;
  margin-left:4rpx;
}
.sponsor>view:nth-child(2){
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.sponsor>view:nth-child(2)>image{
  width: 12rpx;
  height:20rpx;
  margin-top:2rpx;
  margin-left:4rpx;
}

.freelottery{
