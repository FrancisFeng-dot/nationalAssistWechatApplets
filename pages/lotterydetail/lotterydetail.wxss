/* pages/lotterydetail/lotterydetail.wxss */
page {
  width: 100%;
  height: auto;
  overflow-y: auto;
  margin: 0;
  padding: 0;
  background: #f5f5f5;
}

.container {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  position:relative;
}

.goodsdetail{
  width: 100%;
  height:480rpx;
  background: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  position: relative;
}
.goodsdetail>image{
  width: 100%;
  height: 316rpx;
}
.goodsdetail>view:nth-child(2){
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  margin-left: 34rpx;
  margin-bottom: 40rpx;
}
.goodsdetail>view>text:nth-child(1){
  font-family: MicrosoftYaHei;
  font-size:28rpx;
  color:#333333;
}
.goodsdetail>view>text:nth-child(2){
  font-family: MicrosoftYaHei;
  font-size:24rpx;
  color:#666666;
  margin-top: 10rpx;
}
.goodsdetail>view:last-child{
  position: absolute;
  bottom: 0;left: 0;
  right: 0;
  margin: auto;
  width: 100%;
  height: 1px;
  background: #EBEBEB;
  transform: scale(1,0.5);
}

.sponsordetail{
  position:absolute;top:0;right:0;
  font-family:SourceHanSansCN-Regular;
  color:#ffffff;
  font-size:24rpx;
  background-image: url('https://qmzgcdn.boc7.net/test/activeSponsor1.png');
  background-size: 100% 100%;
  box-sizing:border-box;
  padding:16rpx;
  border-top-right-radius:22rpx;
  border-bottom-left-radius:22rpx;
}

.suspensionpiece{
  position:fixed;top:346rpx;right:0;
  background-image:url('https://qmzgcdn.boc7.net/test/lottery_gobackindex2.png');
  background-size:100% 100%;
  z-index:1;
  width:154rpx;height:58rpx;
  text-align:center;
  line-height:56rpx;
  border-bottom-left-radius:32rpx;
  border-top-left-radius:32rpx;
}
.sponsor{
  width: 670rpx;
  height: 76rpx;
  border-radius: 38rpx;
  background: #E2F6E3;
  box-sizing: border-box;
  padding: 0 40rpx;
